<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Nom Activite"
        android:id="@+id/viewHisNom_Activite"
        android:layout_gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="id"
        android:id="@+id/textView38"
        android:layout_marginTop="51dp"
        android:layout_below="@+id/viewHisNom_Activite"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="id"
        android:id="@+id/idHisActivite"
        android:layout_above="@+id/textView39"
        android:layout_alignLeft="@+id/participantHis"
        android:layout_alignStart="@+id/participantHis" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Participant"
        android:id="@+id/textView39"
        android:layout_below="@+id/textView38"
        android:layout_alignLeft="@+id/textView38"
        android:layout_alignStart="@+id/textView38" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/participantHis"
        android:layout_below="@+id/idHisActivite"
        android:layout_toRightOf="@+id/date_finHis"
        android:layout_toEndOf="@+id/date_finHis" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Période"
        android:id="@+id/textView41"
        android:layout_below="@+id/textView39"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/date_debutHis"
        android:layout_alignTop="@+id/date_finHis"
        android:layout_toRightOf="@+id/participantHis"
        android:layout_toEndOf="@+id/participantHis" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Small Text"
        android:id="@+id/date_finHis"
        android:layout_alignTop="@+id/textView41"
        android:layout_toRightOf="@+id/textView39"
        android:layout_toEndOf="@+id/textView39" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="à"
        android:id="@+id/textView40"
        android:layout_alignTop="@+id/date_debutHis"
        android:layout_toRightOf="@+id/idHisActivite"
        android:layout_toEndOf="@+id/idHisActivite" />

</RelativeLayout>